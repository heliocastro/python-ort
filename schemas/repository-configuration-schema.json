    "Curations": {
      "additionalProperties": false,
      "description": "Curations for artifacts in a repository.\n\nAttributes:\n    packages(list[PackageCuration]): Curations for third-party packages.\n    license_findings(list[LicenseFindingCuration]): Curations for license findings.",
      "properties": {
        "packages": {
          "description": "Curations for third-party packages.",
